  <property name="styleSheet">
   <string notr="true">/* =======================================================
   1. VISUAL GERAL (BACKGROUND &amp; FONTE)
   ======================================================= */
QMainWindow {
    background-color: #F8F9FD; /* Fundo quase branco, levemente azulado */
}

QWidget {
    font-family: &quot;Segoe UI&quot;, &quot;Verdana&quot;, sans-serif;
    color: #4A5568; /* Cinza chumbo (muito elegante) */
    font-size: 14px;
}

QFrame #frame_3, #frame_principal, #frame_2, #frame{
	background-color: #F8F9FD;
}

QFrame #frame_5{
	background-color: #F8F9FD;
	border: 3px rgb(218, 218, 218);

}

/* =======================================================
   2. SIDEBAR (MENU LATERAL)
   ======================================================= */
QFrame#frame_main_btn {
    background-color: #FFFFFF;
    border-radius: 0px 30px 30px 0px; /* Curvas suaves na direita */
    border-right: 1px solid #EEF2F7;  /* Separação muito sutil */
}

/* Estilo Base dos Botões do Menu */
QFrame#frame_main_btn QPushButton {
    background-color: transparent;
    color: #A0AEC0; /* Cinza claro para itens inativos */
    font-weight: 600;
    font-size: 18px;
	border: 1px rgba(255, 255, 255, 0);
}

/* Hover (Passar o mouse) */
QFrame#frame_main_btn QPushButton:hover {
    background-color: #EDF2F7;
    color: #5A67D8; /* Roxo suave */
	border: 1px rgba(255, 255, 255, 0);
}

/* Ativo / Selecionado */
QFrame#frame_main_btn QPushButton:pressed {
    /* Gradiente sutil no fundo do botão ativo */
    background-color: rgb(223, 228, 232);
    color: #4C51BF; /* Indigo forte */
    font-weight: bold;
	border: 1px transparent;
}


/* =======================================================
   3. TÍTULOS E TEXTOS
   ======================================================= */
QLabel#label_materias, QLabel#label_semestre, 
QLabel#label_config, QLabel#label_registrar {
    color: #2D3748;
    font-size: 28px;
    font-weight: 800; /* Extra bold */
    padding-bottom: 5px;
}

/* Labels menores dentro dos formulários */
QLabel {
    color: #718096;
    font-size: 18px;
    font-weight: 600;
}

/* =======================================================
   4. CARDS E CONTAINERS (FORMULÁRIOS)
   ======================================================= */
QFrame#frame_estilo_form, 
QFrame#frame_estilo_form_materia, #frame_2 {
    background-color: #F8F9FD;
}

/* =======================================================
   5. INPUTS MODERNOS (ESTILO &quot;FILLED&quot;)
   ======================================================= */
/* Em vez de borda, usamos um fundo cinza que fica branco no foco */
QLineEdit, QSpinBox, QDoubleSpinBox, QComboBox {
    background-color: #EDF2F7; /* Cinza fundo */
    border: 2px solid transparent; /* Sem borda visível inicialmente */
    border-radius: 12px;
    padding: 10px 15px;
    font-size: 15px;
    color: #2D3748;
    font-weight: 500;
}

QLineEdit:focus, QSpinBox:focus, QDoubleSpinBox:focus, QComboBox:focus {
    background-color: #FFFFFF;
    border: 2px solid #667EEA;
    color: #000000;
}

QSpinBox::up-button, QSpinBox::down-button {
    border: none;
    background: transparent;
}

QComboBox#comboBox_semestre {
    background-color: #F7FAFC;
    border: 1px solid #E2E8F0;
    border-radius: 12px;
    padding: 6px 15px;
    color: #4A5568;
    font-size: 15px;
    font-weight: 600;
}

QComboBox#comboBox_semestre:hover {
    background-color: #FFFFFF;
    border-color: #CBD5E0;
}

QComboBox#comboBox_semestre:on {
    background-color: #FFFFFF;
    border-bottom: 0px solid transparent; 
    border-bottom-left-radius: 0px;
    border-bottom-right-radius: 0px;
    padding-bottom: 7px;
}

/* 3. SETA (LIMPA) */
QComboBox#comboBox_semestre::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 30px;
    border: none;
    background: transparent;
}

QComboBox::down-arrow {
    image: url(Icons/arrow_dropdown_icon.png);
	width: 20px;
    height: 20px;
    margin-right: 20px;
	margin-up: 2px;
}
/*lista que abre*/
QComboBox#comboBox_semestre QAbstractItemView {
    background-color: #FFFFFF;
    color: #4A5568;
   	border: 3px #E2E8F0;
	font-size:15px;
    border-radius: 12px;
	margin-left:5px;

}

/* Item Selecionado */
QComboBox#comboBox_semestre QAbstractItemView::item:selected {
    background-color: #EBF4FF;
    color: #5A67D8;
    border-radius: 5px;
}
/* =======================================================
   6. BOTÕES &quot;JUICY&quot; (GRADIENTES VIBRANTES)
   ======================================================= */

/* Botões de Ação Principal (SALVAR) */
QPushButton#btn_salvar_semestre, 
QPushButton#btn_salvar_config, 
QPushButton#btn_salvar_materia {
    /* Gradiente Roxo para Azul */
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 #667EEA, stop:1 #764BA2);
    color: white;
    font-size: 16px;
    font-weight: bold;
    border-radius: 12px;
    padding: 12px;
    border: none;
}

QPushButton#btn_salvar_semestre:hover, 
QPushButton#btn_salvar_config:hover, 
QPushButton#btn_salvar_materia:hover {
    background-color: qlineargradient(spread:pad, x1:0, y1:0, x2:1, y2:1, stop:0 #764BA2, stop:1 #667EEA); /* Inverte o gradiente */
}

QPushButton#btn_salvar_semestre:pressed, 
QPushButton#btn_salvar_config:pressed, 
QPushButton#btn_salvar_materia:pressed {
    background-color: #553C9A;
    padding-top: 14px; /* Efeito de clique */
}

/* Botões Redondos (+) */
QPushButton#btn_registrar_materia, 
QPushButton#btn_add_semestre {
    background-color: #48BB78; /* Verde vibrante */
    color: white;
    border-radius: 20px; /* Redondo (assumindo tam 40-50px) */
}
QPushButton#btn_registrar_materia:hover, 
QPushButton#btn_add_semestre:hover {
    background-color: #38A169;
}

/* Botão de Usuário (Topo) */
QPushButton#btn_usuario {
    background-color: #FFFFFF;
    color: #4A5568;
    border: 1px solid #E2E8F0;
    border-radius: 20px;
    font-weight: 600;
	margin-left:3px;
	margin-right:3px;
}
QPushButton#btn_usuario:hover {
    border: 1px solid #667EEA;
    color: #667EEA;
}


QScrollArea {
    background-color: rgb(248, 249, 253);
	border:none;
}

QScrollBar:vertical {
    background-color: #EDF2F7;
    width: 10px;
    margin: 0px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    background-color: #CBD5E0;
    min-height: 30px;
    border-radius: 5px;
}
QScrollBar::handle:vertical:hover {
    background-color: #A0AEC0;
}
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}

QWidget#scrollArea_materias_conteudo{
    background-color: rgb(248, 249, 253); 
}

QWidget#scrollArea_semestre_conteudo{
    background-color: rgb(248, 249, 253); 
	margin-right: 20px;
}</string>
