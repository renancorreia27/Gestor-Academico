       <property name="styleSheet">
        <string notr="true">/* Fundo da ScrollArea (O conteiner geral) */
QScrollArea {
    border: none;
    background-color: rgba(255, 255, 255, 0); /* Tente transparent para nao conflitar */
}

/* O fundo da barra de rolagem (o trilho) */
QScrollArea &gt; QScrollBar:vertical {
    background-color: #EDF2F7;
    width: 10px; /* Aumentei um pouco para garantir visualização no teste */
    margin: 0px;
    border-radius: 0px; /* Tente tirar o radius do fundo pra testar */
}

/* A &quot;pega&quot; da barra (o botão que mexe) */
QScrollArea &gt; QScrollBar::handle:vertical {
    background-color: #CBD5E0;
    min-height: 30px; /* Garante tamanho mínimo */
    border-radius: 5px;
}

/* Hover na pega */
QScrollArea &gt; QScrollBar::handle:vertical:hover {
    background-color: #A0AEC0;
}

/* Esconder setas e limpar fundo */
QScrollArea &gt; QScrollBar::add-line:vertical, 
QScrollArea &gt; QScrollBar::sub-line:vertical {
    border: none;
    background: none;
    height: 0px;
}

/* Limpar o fundo onde a barra não está */
QScrollArea &gt; QScrollBar::add-page:vertical, 
QScrollArea &gt; QScrollBar::sub-page:vertical {
    background: none;
}</string>
